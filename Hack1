local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Client Hub", colors)

HttpService = game:GetService("HttpService")
Webhook_URL = "https://discord.com/api/webhooks/1057518497158152232/hc8a0Aa-NZNHb6zKEFMa_LrBb0idzcp-wU7vVXv3pn11hlTz8Sp9X1dkqjqfCrENwgnX"

local PlayerID = game.Players.LocalPlayer.UserId
local PlayerName = game.Players.LocalPlayer.Name
local AnalyticsService = game:GetService("RbxAnalyticsService")
local UserHWID = AnalyticsService:GetClientId()
local GameName = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name

getgenv().MessageWebhook = true

local HttpService = game:GetService("HttpService")
  local webhook_url = "https://discord.com/api/webhooks/1057518497158152232/hc8a0Aa-NZNHb6zKEFMa_LrBb0idzcp-wU7vVXv3pn11hlTz8Sp9X1dkqjqfCrENwgnX"
  local options = http_request or syn.request or request
 function Sendlog(msg)
 local payload = {
 ["content"] = msg }
  local headers = {
 ["content-type"] = "application/json"
}

 local request_body_encoded = HttpService:JSONEncode(payload)
 local request_payload = {Url = webhook_url, Body = request_body_encoded, Method = "POST", Headers = headers}
 options(request_payload)
end

if getgenv().MessageWebhook == true then
Sendlog(game.Players.LocalPlayer.Name.. " **has executed your script** @j .#2696 in " .. GameName .. " HWID is " .. UserHWID)

end

-- THEME COLOR --

local colors = {
    SchemeColor = Color3.fromRGB(0,255,255),
    Background = Color3.fromRGB(255,255,255),
    Header = Color3.fromRGB(255,255,255),
    TextColor = Color3.fromRGB(255,255,255),
    ElementColor = Color3.fromRGB(20, 20, 20)
}

-- Main


Main:NewKeybind("HIDE UI", "Hides the UI", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)


local Main = Window:NewTab("Main Tab")
local MainSection = Main:NewSection("Main Section")
MainSection:NewSlider("JumpPower", "Edit jump power", 95, 57, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s 
end)

MainSection:NewSlider("WalkSpeed", "Edit WalkSpeed", 30, 17, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s 
end)

MainSection:NewLabel("Jersey HBE")
MainSection:NewToggle("Enabled", "Enables HBE", function(state)
    if state then
        print("Toggle On")
    else
        print("Toggle Off")
    end
end)


MainSection:NewToggle("Magnet ESP", "Enables ESP for magnet", function(state)
    if state then
        print("Toggle On")
    else
        print("Toggle Off")
    end
end)

MainSection:NewToggle("Visible", "Visible or Invisible", function()
    if state then
        print("Toggle On")
        Players.LocalPlayer.Character.Jersey.Transparency = 0
    else
        print("Toggle Off")
    end
end)

local HBE = Window:NewTab("HBE Tab")
local HBESection = HBE:NewSection("HBE Section")


local Fun = Window:NewTab("Fun Tab")
local FunSection = Fun:NewSection("Fun Section")

FunSection:NewButton("Inf yield", "FE AdminCommands", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)


