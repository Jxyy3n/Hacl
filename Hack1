local old = nil

old = hookmetamethod(game,"__index",function(Self,Key)
  if not checkcaller() and tostring(Self) == "Football" and Key == "Position" then
      return Vector3.new(math.random(1,200),math.random(1,20),math.random(1,200))
    end
    if not checkcaller() and Key == "Velocity" and tostring(Self) == "Head" then
        return Vector3.new(16,0,16)
    elseif not checkcaller() and Key == "Velocity" and tostring(Self) == "HumanoidRootPart" then
        return Vector3.new(16,0,16)
    end
    if not checkcaller() and Key == "Size" and tostring(Self) == "Football" then
        return Vector3.new(0.85, 1.2, 0.85)
    end
    return old(Self,Key)
end)
getgenv().Key = "Click" -- Click is left click, other than that it can be any other alphabetical key on your keyboard
getgenv().Range = 0


local function ball()
    return workspace:FindFirstChild("Handle")
end
local function getsetting()
    local key = string.upper(getgenv().Key)
    return ((getgenv().Key == "Click" and Enum.UserInputType.MouseButton1) or (Enum.KeyCode[key]))
end

local uis = game:GetService("UserInputService");
local plr = game.Players.LocalPlayer;

uis.InputBegan:Connect(function(key,gp)
    if gp then return end;

    if key.UserInputType == getsetting() or key.KeyCode == getsetting() then
        print("Cool man")


        for i = 1,65 do wait()
            local b = ball()
            if b and plr.Character then
                local magnitude = (plr.Character:FindFirstChild("HumanoidRootPart").Position - b.Position).Magnitude;
                if magnitude <= getgenv().Range then
                    b.CanCollide = false
                    b.CFrame = CFrame.new(plr.Character:FindFirstChild("HumanoidRootPart").Position)
                    firetouchinterest(b,plr.Character:FindFirstChild("HumanoidRootPart"),0)
                    task.wait()
                    firetouchinterest(b,plr.Character:FindFirstChild("HumanoidRootPart"),1)
                end
            end
        end
    end
end)
